Public Class Form1
    Private Sub Button1_Click_1(sender As Object, e As EventArgs) Handles Button1.Click
        Dim selectedOptions As String = "Selected Preferences: " & vbCrLf
        If CheckBox1.Checked Then
            selectedOptions &= " C " & vbCrLf
        ElseIf CheckBox2.Checked Then
            selectedOptions &= "Python " & vbCrLf
        ElseIf CheckBox3.Checked Then
            selectedOptions &= "Java" & vbCrLf
        End If
        If RadioButton1.Checked Then
            selectedOptions &= "Male" & vbCrLf
        ElseIf RadioButton2.Checked Then
            selectedOptions &= "Female" & vbCrLf
        ElseIf RadioButton3.Checked Then
            selectedOptions &= "Other" & vbCrLf
        End If
        Label1.Text = selectedOptions

    End Sub
End Class
